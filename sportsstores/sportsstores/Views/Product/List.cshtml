@model ProductsListViewModel

@foreach (var p in Model.products) {
    @Html.Partial("ProductSummary", p);
}  
<div page-model="@Model.padingInfo" page-action="List" page-classes-enable="true"
     page-class="btn" page-classes-normal="btn-secondary" 
     page-class-select="btn-primary" page-url-category="@Model.CurrentCategory"
    class="btn-group pull-right m-1">
</div>